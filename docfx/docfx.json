{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Cake.AzCopy/*.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "docfx/**"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "netstandard1.6"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "doc/**.md",
          "doc/**/toc.yml"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "assets/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Cake.AzCopy",
      "_enableSearch": true,
      "_appLogoPath": "assets/appIcon.png",
      "_appFaviconPath": "assets/favicon.ico",
      "_appFooter": "This addin is Â© 2017 Alistair Chapman, and released under the terms of the MIT License"
    },
    "template": [
      "statictoc"
    ],
    "noLangKeyword": false
  }
}